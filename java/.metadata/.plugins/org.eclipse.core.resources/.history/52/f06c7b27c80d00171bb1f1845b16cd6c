package scagnostics;

public class Exp extends Thread
{
	public void run()
	{
		for(int i = 0; i < 3; i++)
		{
			System.out.println(i);
		}
	}
	public static void main(String[] args) throws InterruptedException
	{
		System.out.println("Exp");
		Exp e = new Exp();
		Exp a = new Exp();
		a.start();
		e.start();
		e.join();
		System.out.println("OVER");
	}
}
